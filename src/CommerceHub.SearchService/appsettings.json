{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "mongoDb": "mongodb://localhost:27017",
    "MongoDatabaseName": "commercehub_search",
    "rabbitMq": "amqp://localhost"
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "AuthorizationUrl": "http://localhost:8080/realms/commerce-hub/protocol/openid-connect/auth",
    "TokenUrl": "http://localhost:8080/realms/commerce-hub/protocol/openid-connect/token"
  },
  "Authentication": {
    "MetadataAddress": "http://localhost:8080/realms/commerce-hub/.well-known/openid-configuration",
    "ValidIssuer": "http://localhost:8080/realms/commerce-hub",
    "ValidAudience": "account"
  }
}
